<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 게시판 처리 SQL -->
<mapper namespace="global.sesoc.newdoc.DAO.NewdocMapper">
	<insert id="createnewdoc" parameterType="Newdoc">
		INSERT INTO NEW_DOC
		(
		USERNUM
		)
		VALUES
		(
		#{usernum}
		)
	</insert>

	<insert id="test" parameterType="String">
		INSERT INTO NEW_DOC
		(
		USERNUM
		)
		VALUES
		(
		#{param}
		)
	</insert>

</mapper>
